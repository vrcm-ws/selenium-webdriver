<?xml version="1.0" encoding="UTF-8"?>
<Configuration status="WARN">

    <Appenders>
        <Console name="CONSOLE" target="SYSTEM_OUT">
            <PatternLayout pattern="[%t] %d{HH:mm:ss} %-5p [%c{1}] %m%n"/>
        </Console>
    </Appenders>

    <Loggers>
        <Root level="TRACE">
            <AppenderRef ref="CONSOLE"/>
        </Root>
    </Loggers>

</Configuration>